<%- include("./includes/head.ejs") %>

</head>
<body>
    
    
    
    <div class="main-text grey lighten-4">
        <%- include("./includes/navbar.ejs") %>
        <div class="container">
            <% for (let dog of dogs) { %>
                <div class="row">
                    <div class="col s12 m5">
                      <div class="card">
                        <div class="card-image">
                          <img src="<%= dog.imageUrl %>">
                          <a class="btn-floating halfway-fab waves-effect waves-light red"><i class="material-icons"></i></a>
                        </div>
                        <div class="card-content">
                        <span class="card-title activator grey-text text-darken-4"><%= dog.breed %></span>
                          <p> <%= dog.description %></p>
                        </div>
                      </div>
                    </div>
                  </div>
            <% } %>
        </div>
    </div>

</body>
</html>